<site root="https://example.com" authenticated="false">
  <page url="/images?page={index}" start="1">
    <entry regex="((/photos/[^&quot;]+)&quot; class=&quot;o)" enumerate="all">
      <page url="{1}" save-progress="true">
        <entry regex="((https[^&quot;]+)&quot; class=&quot;download)" enumerate="0">
          <action type="download" url="{1}" />
        </entry>
      </page>
    </entry>
  </page>
  <page url="/videos?page={index}" start="1">
    <entry regex="((/videos/[^&quot;]+)&quot; class=&quot;p)" enumerate="all">
      <page url="{1}" save-progress="true">
        <entry regex="((https[^&quot;]+)&quot; class=&quot;download)" enumerate="0">
          <action type="download" url="{1}" />
        </entry>
      </page>
    </entry>
  </page>
</site>
